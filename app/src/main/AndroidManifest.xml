    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name="sonar2.tistory.com.busan.easydisabledemploy.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="sonar2.tistory.com.busan.easydisabledemploy.ChargeActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="sonar2.tistory.com.busan.easydisabledemploy.IntroActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="sonar2.tistory.com.busan.easydisabledemploy.KeadIntroActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="sonar2.tistory.com.busan.easydisabledemploy.Keadintro2Activity"
            android:screenOrientation="landscape" />
        <activity
            android:name="sonar2.tistory.com.busan.easydisabledemploy.Keadintro3Activity"
            android:screenOrientation="landscape" />
        <activity android:name="sonar2.tistory.com.busan.easydisabledemploy.config">

        </activity>
    </application>



</manifest>