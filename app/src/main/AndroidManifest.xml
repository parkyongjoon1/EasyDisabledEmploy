<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.or.kead.busan.easydisabledemploy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChargeActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".IntroActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".KeadIntroActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".Keadintro2Activity" />
        <activity android:name=".Keadintro3Activity"/>
        <activity android:name=".config">

        </activity>
    </application>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


</manifest>