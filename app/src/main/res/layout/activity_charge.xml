        android:paddingEnd="@dimen/_20sdp"
        android:contentDescription="@string/chargetitle"
        tools:ignore="RtlSymmetry">

        <ImageView
            android:src="@drawable/png_btn_set"
            android:id="@+id/btn_config"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_20sdp"
            android:layout_margin="@dimen/_1sdp"
            android:onClick="onClickConfig"
            android:text="@string/setting"
            android:contentDescription="@string/setting" />

        <ImageView
            android:src="@drawable/png_btn_save"
            android:id="@+id/btn_save"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_20sdp"
            android:layout_margin="@dimen/_1sdp"
            android:onClick="onClickSave"
            android:contentDescription="@string/save" />

        <ImageView
            android:src="@drawable/png_btn_open"
            android:id="@+id/btn_load"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_20sdp"
            android:layout_margin="@dimen/_1sdp"
            android:onClick="onClickLoad"
            android:contentDescription="@string/load" />

        <ImageView
            android:src="@drawable/png_btn_del"
            android:id="@+id/btn_del"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_20sdp"
            android:layout_margin="@dimen/_1sdp"
            android:onClick="onClickDel"
            android:contentDescription="@string/delete" />
    </LinearLayout>



